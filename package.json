    "build:server:prod": "node_modules/.bin/cross-env NODE_ENV=production node_modules/.bin/webpack --config webpack.config.server.ts",
    "build:server": "node_modules/.bin/cross-env NODE_ENV=development node_modules/.bin/webpack --config webpack.config.server.ts",
    "start": "nf start",
    "start:prod": "cross-env NODE_ENV=production node dist/server.bundle.js",
    "b": "npm run clean && npm run build && npm run build:server && npm run start",
    "b:prod": "npm run clean && npm run build:prod && npm run build:server:prod && npm run start:prod",
    "clean": "node_modules/.bin/rimraf dist"
  },
